{
  "name": "colbycomms/wp-schedule",
  "description": "A WordPress plugin for creating table-based schedules.",
  "type": "library",
  "scripts": {
    "phpcbf": "phpcbf --extensions=php",
    "lint": "phpcs --extensions=php -s",
    "test": "phpunit",
    "post-install-cmd": "yarn"
  },
  "authors": [
    {
      "name": "John Watkins",
      "email": "john.watkins@colby.edu"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "htmlburger/carbon-fields": "^2.1",
    "phpunit/phpunit": "*",
    "colbycomms/wp-collapsible": "dev-master",
    "colbycomms/colby-svg": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ColbyComms\\Schedules\\": "wp-schedule/"
    },
    "files": ["wp-schedule/index.php"]
  }
}
